process_fields(&$n){$he=0;if($_POST["up"]){$td=0;foreach($n
as$z=>$m){if(key($_POST["up"])==$z){unset($n[$z]);array_splice($n,$td,0,array($m));break;}if(isset($m["field"]))$td=$he;$he++;}}elseif($_POST["down"]){$Ac=false;foreach($n
